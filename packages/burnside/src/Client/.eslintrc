{
  "extends": "eslint-config-airbnb-es5",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "Promise": true
  },
  "rules": {
    "new-cap": off,
    "no-param-reassign": "off",
    "no-new-func": "off",
    "no-extend-native": "off",
    "no-shadow": "off"
  }
}
